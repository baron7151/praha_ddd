datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

generator client {
  previewFeatures = ["nativeTypes"]
  provider        = "prisma-client-js"
}

model SomeData {
  id        String    @id
  required Boolean
  number   Int     @db.SmallInt
}

model User {
  id   Int  @id @default(autoincrement())
  userId   String @unique
  userName String
  email String   @unique
  status String
  pairId String?
  teamId String?
  taskProgressId String?
}
