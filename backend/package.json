{
  "name": "deploy-sample",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": "18.x.x"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "gcp-build": "yarn build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "STAGE=production node dist/src/main",
    "dev": "dotenv -e .local.env -- nest start --watch",
    "dev:debug": "dotenv -e .local.env -- nest start --debug --watch",
    "dev:test": "dotenv -e .test.env -- nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "yarn lint --fix",
    "migrate:dev": "dotenv -e .local.env -- prisma migrate dev",
    "model-generate": "prisma generate",
    "migrate:dev:reset": "dotenv -e .local.env -- prisma migrate reset",
    "migrate:test": "dotenv -e .test.env -- prisma migrate reset --force",
    "migrate:test2": "dotenv -e .test.env -- prisma migrate dev",
    "migrate:prd": "prisma migrate deploy --preview-feature",
    "test:unit": "dotenv -e .test.env -- jest",
    "test:integration": "yarn migrate:test && dotenv -e .test.env -- jest -c ./jest.integration.config.js --runInBand",
    "test:api": "yarn migrate:test && dotenv -e .test.env -- jest -c ./jest.api.config.js --runInBand"
  },
  "dependencies": {
    "@nestjs/common": "^9.4.0",
    "@nestjs/core": "^9.4.0",
    "@nestjs/platform-express": "^9.4.0",
    "@nestjs/swagger": "^6.3.0",
    "@prisma/client": "^4.14.0",
    "axios": "^1.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "csv": "^6.3.0",
    "dotenv": "^16.0.3",
    "inquirer": "^9.2.5",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.0",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^4.6.3",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.1",
    "@nestjs/cli": "^9.4.2",
    "@nestjs/schematics": "^9.1.0",
    "@nestjs/testing": "^9.4.0",
    "@types/express": "^4.17.17",
    "@types/faker": "^6.6.9",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.1.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "faker": "^6.6.6",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "prisma": "^4.14.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  }
}
